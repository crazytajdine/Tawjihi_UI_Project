{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/components/NavFooter.jsx", "app/components/NavUpperbarre.jsx", "app/layout.jsx", "app/page.jsx", "app/api/Events/route.jsx", "app/api/other/location/route.jsx", "app/api/School/route.jsx", "app/components/main_Components/TableSearch/Navmain.jsx", "app/components/main_Components/TableSearch/SearchTable.jsx", "app/components/main_Components/TableSearch/SpotTableSearch.jsx", "app/components/main_Components/Sponsors/Sponsors.jsx", "app/components/main_Components/Qustions/Questions.jsx", "app/components/main_Components/Events/CardEvent.jsx", "app/components/main_Components/Events/Events.jsx", "app/components/main_Components/Events/LabelEvents.jsx"],
  "exclude": ["node_modules"]
}
